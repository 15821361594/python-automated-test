self.p_expr[0] = logcombine(self.p_expr[1]) 
self.p_term[1] = self.p_c[2] * self.p_v[0] ** self.p_c[3] 
self.p_expr[2] = Sum(self.p_expr[3],(self.p_v[2],self.p_c[3],self.p_c[0])) 
