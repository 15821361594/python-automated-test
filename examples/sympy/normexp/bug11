self.p_c[3] = Integer(3) 
self.p_v[0] = Symbol('k',positive=True) 
self.p_c[0] = Integer(6) 
self.p_v[2] = Symbol('k',positive=True) 
self.p_expr[3] = Rational(self.p_c[0],self.p_c[3]) 
self.p_c[2] = Integer(3) 
self.p_expr[3].evalf() 
self.p_expr[3] = Rational(self.p_c[2],self.p_c[2]) 
self.p_expr[1] = expand_power_exp(self.p_expr[3]) 
self.p_expr[2] = self.p_expr[3].subs(self.p_expr[1],self.p_expr[1]) 
