self.p_c[3] = Integer(6) 
self.p_c[1] = Integer(8) 
self.p_expr[2] = E 
self.p_expr[0] = expand_power_base(self.p_expr[2]) 
self.p_v[1] = Symbol('z') 
self.p_v[3] = Symbol('x') 
self.p_expr[2] = trigsimp(self.p_expr[2]) 
self.p_expr[1] = Sum(self.p_expr[2],(self.p_v[3],self.p_c[3],self.p_c[1])) 
self.p_v[0] = Symbol('z') 
self.p_expr[3] = self.p_expr[0] % self.p_expr[1] 
