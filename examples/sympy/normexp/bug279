self.p_c[2] = Integer(5) 
self.p_v[2] = Symbol('y') 
self.p_term[2] = self.p_c[2] * self.p_v[2] ** self.p_c[2] 
self.p_c[0] = Integer(10) 
self.p_expr[2] = E 
self.p_expr[1] = self.p_expr[2] ** self.p_expr[2] 
self.p_expr[3] = self.p_expr[2].subs(self.p_expr[2],self.p_expr[2]) 
self.p_expr[2] = Product(self.p_expr[1],(self.p_v[2],self.p_c[0],self.p_c[2])) 
self.p_expr[1] = self.p_expr[2].subs(self.p_expr[2],self.p_expr[1]) 
self.p_expr[1] = self.p_expr[3] % self.p_expr[2] 
