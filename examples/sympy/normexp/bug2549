self.p_c[2] = Integer(8) 
self.p_expr[1] = E 
self.p_c[3] = Integer(0) 
self.p_expr[2] = E 
self.p_v[3] = Symbol('d') 
self.p_expr[0] = cancel(self.p_expr[2]) 
self.p_expr[2] = acos(self.p_expr[0]) 
self.p_expr[0] = logcombine(self.p_expr[0]) 
self.p_expr[3] = self.p_expr[1] ** self.p_expr[0] 
self.p_expr[0] = self.p_expr[1].subs(self.p_expr[0],self.p_expr[3]) 
self.p_expr[3] = cancel(self.p_expr[3]) 
self.p_expr[1] = self.p_expr[3].subs(self.p_expr[1],self.p_expr[3]) 
