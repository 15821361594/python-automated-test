self.p_v[0] = Symbol('z') 
self.p_v[1] = Symbol('y') 
self.p_c[2] = Integer(9) 
self.p_term[3] = self.p_c[2] * self.p_v[0] ** self.p_c[2] 
self.p_poly[3] = self.p_term[3] + self.p_term[3] 
self.p_v[3] = Symbol('i',positive=True) 
self.p_poly[2] = self.p_term[3] 
self.p_expr[0] = E 
self.p_expr[3] = self.p_expr[0] ** self.p_expr[0] 
self.p_c[2] = Integer(9) 
self.p_expr[2] = Product(self.p_expr[0],(self.p_v[3],self.p_c[2],self.p_c[2])) 
self.p_expr[0] = self.p_expr[3] % self.p_expr[2] 
