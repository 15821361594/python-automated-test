self.p_v[1] = Symbol('l',positive=True) 
self.p_v[0] = Symbol('l',positive=True) 
self.p_c[2] = Integer(7) 
self.p_expr[0] = Rational(self.p_c[2],self.p_c[2]) 
self.p_expr[1] = cancel(self.p_expr[0]) 
self.p_expr[2] = self.p_expr[1] + self.p_expr[1] 
self.p_expr[3] = self.p_expr[2] + self.p_expr[0] 
self.p_expr[2] = atan(self.p_expr[3]) 
self.p_expr[1] = Sum(self.p_expr[0],(self.p_v[0],self.p_c[2],self.p_c[2])) 
self.p_expr[3] = self.p_expr[1] % self.p_expr[2] 
