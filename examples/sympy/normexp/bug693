self.p_v[1] = Symbol('x') 
self.p_c[3] = Integer(4) 
self.p_v[0] = Symbol('k',positive=True) 
self.p_v[2] = Symbol('x') 
self.p_expr[3] = self.p_v[2] 
self.p_expr[0] = Product(self.p_expr[3],(self.p_v[2],self.p_c[3],self.p_c[3])) 
self.p_expr[3] = Sum(self.p_expr[0],(self.p_v[0],self.p_c[3],self.p_c[3])) 
self.p_v[0] = Symbol('a') 
self.p_c[0] = Integer(4) 
self.p_expr[2] = Sum(self.p_expr[0],(self.p_v[2],self.p_c[0],self.p_c[0])) 
self.p_expr[1] = self.p_expr[2] % self.p_expr[0] 
