self.p_c[0] = Integer(8) 
self.p_c[2] = Integer(5) 
self.p_v[0] = Symbol('c') 
self.p_expr[1] = Rational(self.p_c[0],self.p_c[2]) 
self.p_c[2] = Integer(2) 
self.p_expr[2] = Sum(self.p_expr[1],(self.p_v[0],self.p_c[0],self.p_c[0])) 
self.p_expr[3] = expand_func(self.p_expr[2]) 
self.p_expr[1] = self.p_expr[2].subs(self.p_expr[2],self.p_expr[3]) 
self.p_v[3] = Symbol('z') 
self.p_expr[3] = cos(self.p_expr[3]) 
self.p_expr[0] = self.p_expr[3] % self.p_expr[2] 
