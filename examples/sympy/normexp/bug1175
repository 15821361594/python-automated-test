self.p_v[3] = Symbol('b') 
self.p_c[1] = Integer(8) 
self.p_expr[1] = pi 
self.p_expr[3] = Rational(self.p_c[1],self.p_c[1]) 
self.p_expr[2] = self.p_expr[3].doit() 
self.p_expr[3] = acos(self.p_expr[1]) 
self.p_c[2] = Integer(2) 
self.p_expr[1] = self.p_expr[2] + self.p_expr[1] 
self.p_expr[0] = self.p_expr[3].subs(self.p_expr[1],self.p_expr[1]) 
self.p_c[3] = Integer(2) 
self.p_expr[1] = self.p_expr[2] % self.p_expr[3] 
