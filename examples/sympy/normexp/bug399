self.p_v[1] = Symbol('a') 
self.p_c[0] = Integer(2) 
self.p_c[3] = Integer(3) 
self.p_expr[2] = self.p_v[1] 
self.p_expr[3] = self.p_c[0] 
self.p_expr[1] = self.p_expr[3].subs(self.p_expr[2],self.p_expr[3]) 
self.p_expr[0] = Product(self.p_expr[2],(self.p_v[1],self.p_c[0],self.p_c[3])) 
self.p_expr[3] = self.p_expr[3] * self.p_expr[0] 
self.p_expr[0] = self.p_c[3] 
self.p_expr[2] = self.p_expr[0] % self.p_expr[3] 
