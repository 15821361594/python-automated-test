self.p_v[0] = Symbol('c') 
self.p_c[3] = Integer(1) 
self.p_v[2] = Symbol('a') 
self.p_c[1] = Integer(8) 
self.p_expr[1] = Rational(self.p_c[3],self.p_c[3]) 
self.p_expr[2] = Product(self.p_expr[1],(self.p_v[0],self.p_c[3],self.p_c[1])) 
self.p_v[3] = Symbol('x') 
self.p_expr[3] = factor(self.p_expr[1]) 
self.p_term[2] = self.p_c[1] * self.p_v[0] ** self.p_c[3] 
self.p_expr[1] = self.p_expr[3] % self.p_expr[2] 
