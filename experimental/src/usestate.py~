import sut
import random

t = sut.t()

LEN = 2000
N = 1000

states = []

for n in xrange(0,N):
    for s in xrange(0,LEN):
        if t.state() not in states:
            print "NEW STATE!!!",t.state()
            states.append(t.state())
        action = random.choice(t.enabled())
        action[2]()
        assert(t.check())
