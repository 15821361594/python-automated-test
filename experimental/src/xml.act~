@import xml.dom.minidom

pool: %DOMTREE% 3
pool: %STRING% 3
pool: %COLLECTION% 3
pool: %CHAR% 3
pool: %QUERY% 3

%STRING% := ""
~%STRING% += %CHAR%
%CHAR% := "<"
%CHAR% := ">"
%CHAR% := "a"
%CHAR% := "b"
%CHAR% := "c"
%DOMTREE% := xml.dom.minidom.parseString(%STRING%)
%COLLECTION% := %DOMTREE%.documentElement
%QUERY% := %COLLECTION%.getElementsByTagName(%STRING%)